 Times PSNFSS2e package plus Times in math]

%%% Set default sans serif font to Helvetica,
%%% default serifed font to Times, and
%%% default typewriter font to Courier:
\renewcommand{\sfdefault}{phv}
\renewcommand{\rmdefault}{ptm}
\renewcommand{\ttdefault}{pcr}

%%% Change math alphabet identifiers to use Times:
\DeclareMathAlphabet{\mathrm}    {OT1}{ptm}{m}{n}
\DeclareMathAlphabet{\mathbf}    {OT1}{ptm}{b}{n}
\DeclareMathAlphabet{\mathsf}    {OT1}{phv}{m}{n}
\DeclareMathAlphabet{\mathtt}    {OT1}{pcr}{m}{n}
\DeclareMathAlphabet{\mathnormal}{OT1}{ptm}{m}{it}
\DeclareMathAlphabet{\mathit}    {OT1}{ptm}{m}{it}
\DeclareMathAlphabet{\mathbit}   {OT1}{ptm}{b}{it}

%%% Change math operators (log, sin, and so on) to use Times:
\DeclareSymbolFont{operators}    {OT1}{ptm}{m}{n}
\SetSymbolFont{operators}{bold}  {OT1}{ptm}{b}{n}

%%% Declare new symbol fonts to be used for punctuation and letters:
\DeclareSymbolFont{times}        {OT1}{ptm}{m}{n}
\SetSymbolFont{times}{bold}      {OT1}{ptm}{b}{n}
\DeclareSymbolFont{timesit}      {OT1}{ptm}{m}{it}
\SetSymbolFont{timesit}{bold}    {OT1}{ptm}{b}{it}

